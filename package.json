{
  "name": "qoloba-proxy",
  "version": "1.0.0",
  "description": "OpenAI-compatible proxy server for Qolaba API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "build": "echo 'No build step required for Node.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:header-fix": "node tests/test-runner.js all",
    "test:header-fix-suite": "node tests/test-runner.js suite",
    "test:header-fix-manual": "node tests/test-runner.js manual",
    "test:header-fix-original": "node tests/test-original-error-scenarios.js",
    "test:header-fix-response": "node tests/test-fixed-response-management.js",
    "test:header-fix-edge": "node tests/test-edge-cases.js",
    "test:header-fix-logging": "node tests/test-logging-functionality.js",
    "test:header-fix-performance": "node tests/test-performance.js",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "docker:build": "docker build -t qoloba-proxy .",
    "docker:run": "docker run -p 3000:3000 --env-file .env qoloba-proxy"
  },
  "keywords": [
    "openai",
    "proxy",
    "qolaba",
    "api",
    "llm",
    "chat",
    "streaming"
  ],
  "author": "Qoloba Proxy Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/qoloba-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/qoloba-proxy/issues"
  },
  "homepage": "https://github.com/your-org/qoloba-proxy#readme"
}